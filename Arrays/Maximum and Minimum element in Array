class Solution
{ 
    public static int findSum(int A[],int N) 
    {
        int min=99999999;
        int max=-99999999; 
        for(int i=0;i<N;i++){
            if(A[i]<min) 
               min=A[i];
            if(A[i]>max)  
               max=A[i];
               
        } 
        return min+max;
    }
}
